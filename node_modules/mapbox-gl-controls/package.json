{
  "name": "mapbox-gl-controls",
  "version": "2.3.5",
  "main": "./lib/index.js",
  "description": "Controls for mapbox-gl",
  "scripts": {
    "lint": "eslint src --ext .js,.ts",
    "watch": "tsc --watch & sass --watch src/index.scss:lib/controls.css",
    "build": "tsc && sass src/index.scss:lib/controls.css",
    "docs": "cp lib/controls.css docs && rollup --config docs/rollup.config.js",
    "docs:watch": "cp lib/controls.css docs && rollup --config docs/rollup.config.js --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/geojson": "^7946.0.7",
    "@types/mapbox-gl": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-xxx": "^1.0.0",
    "mapbox-gl": "^2.3.0",
    "rollup": "^2.51.1",
    "sass": "^1.34.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@turf/distance": "6.3.0"
  },
  "peerDependencies": {
    "mapbox-gl": ">=1.0.0 <3.0.0"
  },
  "homepage": "https://korywka.github.io/mapbox-gl-controls/",
  "repository": "korywka/mapbox-gl-controls",
  "keywords": ["mapbox"],
  "license": "MIT"
}
